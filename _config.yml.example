# Logo Url
# Please enter an absolute path to your logo file
# or remove the following line with replacing logo-header.png with your own logo
logo:

# Header
menu:
  Home: /
  # Delete this row if you don't want categories in your header nav bar
  Cates:
  About: /about/index.html

# Article list grouped by year
groupByYear: true

# Content
fancybox: true

# Thumbnail
thumbnail: true

# Scroll Loading
scrollLoading: true

# Sidebar
sidebar: right # sidebar's position (left or right)
social_links:
  twitter:
  facebook:
  google-plus:
  github: https://github.com/o2team/o2team.github.io
  weibo:
  rss:
widgets:
- recent_posts
- category
- archive
- tag
- tagcloud
- links

# Links
links:
  Hexo: http://hexo.io

# Don't Use Google APIs if you are in google-blocked areas
use_google_apis: false

# Miscellaneous
google_analytics:
baidu_analytics:
favicon: /favicon.png
fb_admins:
fb_app_id:
